<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.Dimcooo.model.SubjectFeedback" table="subject_feedback" schema="training_center">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="mark">
            <column name="mark" sql-type="int(5)"/>
        </property>
        <property name="description">
            <column name="description" sql-type="varchar(255)"/>
        </property>
        <many-to-one name="scholarByScholarId" class="com.Dimcooo.model.Scholar">
            <column name="Scholar_id"/>
        </many-to-one>
        <many-to-one name="subjectBySubjectId" class="com.Dimcooo.model.Subject">
            <column name="Subject_id"/>
        </many-to-one>
    </class>
</hibernate-mapping>