<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.Dimcooo.model.SubjectScholarList" table="subject_scholar_list" schema="training_center">
        <id name="id">
            <column name="ID" sql-type="int(11)"/>
        </id>
        <property name="registration">
            <column name="Registration" sql-type="date"/>
        </property>
        <property name="expiration">
            <column name="Expiration" sql-type="date"/>
        </property>
    </class>
</hibernate-mapping>