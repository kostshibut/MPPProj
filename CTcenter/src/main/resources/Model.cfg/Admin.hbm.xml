<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.Dimcooo.model.Admin" table="admin" schema="ct_center">
        <id name="adminId">
            <column name="admin_id" sql-type="int(11)"/>
        </id>
        <property name="userUserId">
            <column name="user_user_id" sql-type="int(11)"/>
        </property>
        <many-to-one name="userByUserUserId" class="com.Dimcooo.model.User" insert="false" update="false">
            <column name="user_user_id"/>
        </many-to-one>
    </class>
</hibernate-mapping>