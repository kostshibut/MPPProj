<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.Dimcooo.model.TeacherFeedback" table="teacher_feedback" schema="ct_center">
        <id name="teacherFeedbackId">
            <column name="teacher_feedback_id" sql-type="int(11)"/>
        </id>
        <property name="mark">
            <column name="mark" sql-type="int(5)" not-null="true"/>
        </property>
        <property name="description">
            <column name="description" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="scholarScholarId" insert="false" update="false">
            <column name="scholar_scholar_id" sql-type="int(11)"/>
        </property>
        <property name="teacherTeacherId" insert="false" update="false">
            <column name="teacher_teacher_id" sql-type="int(11)"/>
        </property>
        <many-to-one name="scholarByScholarScholarId" class="com.Dimcooo.model.Scholar">
            <column name="scholar_scholar_id"/>
        </many-to-one>
        <many-to-one name="teacherByTeacherTeacherId" class="com.Dimcooo.model.Teacher">
            <column name="teacher_teacher_id"/>
        </many-to-one>
    </class>
</hibernate-mapping>