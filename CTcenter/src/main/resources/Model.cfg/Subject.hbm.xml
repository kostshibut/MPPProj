<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.Dimcooo.model.Subject" table="subject" schema="ct_center">
        <id name="subjectId">
            <column name="subject_id" sql-type="int(11)"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="duration">
            <column name="duration" sql-type="int(11)"/>
        </property>
        <property name="teacherTeacherId">
            <column name="teacher_teacher_id" sql-type="int(11)"/>
        </property>
        <many-to-one name="teacherByTeacherTeacherId" insert="false" update="false" cascade="save-update" class="com.Dimcooo.model.Teacher">
            <column name="teacher_teacher_id"/>
        </many-to-one>
        <set name="lessonsBySubjectId" cascade="save-update" inverse="true">
            <key>
                <column name="subject_subject_id"/>
            </key>
            <one-to-many not-found="ignore" class="com.Dimcooo.model.Lesson"/>
        </set>
        <set name="listScholarSubjectsBySubjectId" cascade="save-update" inverse="true">
            <key>
                <column name="subject_subject_id"/>
            </key>
            <one-to-many not-found="ignore" class="com.Dimcooo.model.ListScholarSubject"/>
        </set>
    </class>
</hibernate-mapping>