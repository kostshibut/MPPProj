<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.Dimcooo.model.ListScholarSubject" table="list_scholar_subject" schema="ct_center">
        <id name="listScholarSubjectId">
            <column name="list_scholar_subject_id" sql-type="int(11)"/>
        </id>
        <property name="scholarScholarId">
            <column name="scholar_scholar_id" sql-type="int(11)"/>
        </property>
        <property name="subjectSubjectId">
            <column name="subject_subject_id" sql-type="int(11)"/>
        </property>
        <many-to-one name="scholarByScholarScholarId" insert="false" update="false" class="com.Dimcooo.model.Scholar">
            <column name="scholar_scholar_id"/>
        </many-to-one>
        <many-to-one name="subjectBySubjectSubjectId" insert="false" update="false" class="com.Dimcooo.model.Subject">
            <column name="subject_subject_id"/>
        </many-to-one>
    </class>
</hibernate-mapping>