<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.Dimcooo.model.SubjectFeedback" table="subject_feedback" schema="ct_center">
        <id name="subjectFeedbackId">
            <column name="subject_feedback_id" sql-type="int(11)"/>
        </id>
        <property name="mark">
            <column name="mark" sql-type="int(5)" not-null="true"/>
        </property>
        <property name="description">
            <column name="description" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="subjectSubjectId">
            <column name="subject_subject_id" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="scholarScholarId">
            <column name="scholar_scholar_id" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>